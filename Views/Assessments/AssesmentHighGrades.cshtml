@model IEnumerable<Milestone__3.Models.HighestAssesment>

@{
    ViewData["Title"] = "Highest Grades";
}

<h1>Highest Grades</h1>

<table class="table">
    <thead>
        <tr>
            <th>Course ID</th>
            <th>Assesment Title</th>
            <th>Highest Marks</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.CourseID</td>
                <td>@item.title</td>
                <td>@item.HighestMarks</td>
            </tr>
        }
    </tbody>
</table>
